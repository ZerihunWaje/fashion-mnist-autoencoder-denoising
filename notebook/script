      "cell_type": "markdown",
      "source": [
        "**Build and Train Autoencoder Deep learning model**"
      ],
      "metadata": {
        "id": "HFl6bVqNVmlO"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "autoencoder = tf.keras.models.Sequential()\n",
        "\n",
        "#encoder\n",
        "autoencoder.add(tf.keras.layers.Conv2D(filters=16, kernel_size=3, strides=2, padding='same', input_shape=(28, 28, 1)))\n",
        "autoencoder.add(tf.keras.layers.Conv2D(filters=8, kernel_size=3, strides=2, padding='same'))#, input_shape=(28, 28, 1)))\n",
        "\n",
        "autoencoder.add(tf.keras.layers.Conv2D(filters=16, kernel_size=3, strides=1, padding='same'))#, input_shape=(28, 28, 1)))\n",
        "\n",
        "#decoder\n",
        "autoencoder.add(tf.keras.layers.Conv2DTranspose(filters=16, kernel_size=3, strides=2, padding='same'))#, input_shape=(28, 28, 1)))\n",
        "autoencoder.add(tf.keras.layers.Conv2DTranspose(filters=1, kernel_size=3, strides=2, activation='sigmoid', padding='same'))"
      ],
      "metadata": {
        "id": "E7y_RPeaVgx7"
      },
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "autoencoder.compile(loss='binary_crossentropy', optimizer=tf.keras.optimizers.Adam(lr=0.001))\n",
        "autoencoder.summary()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "JH60l4AxVzV3",
        "outputId": "9943d40f-ad25-441d-d57b-dc66aea4397e"
      },
      "execution_count": 19,
      "outputs": [
        {
