      }
    },
    {
      "cell_type": "code",
      "source": [
        "evaluation = autoencoder.evaluate(noise_test_dataset.reshape(-1, 28, 28, 1), X_test.reshape(-1, 28, 28, 1))\n",
        "print('Test Loss : {:.3f}'.format(evaluation))"
      ],
      "metadata": {
        "colab": {
